/*
 * Link: https://sites.google.com/site/verilog710/xiang-guan-gong-ju/icarus-verilog
 */
iverilog -o sample.vvp sample_test.v sample.v  // Generate sample.vvp
vvp sample.vvp  // Execute.


Under "src/verilog" directory.

/*
 * Not gate.
 */
iverilog -o Not_test.vvp Not_test.v Not.v  // Generate Not_test.vvp
vvp Not_test.vvp  // Execute.
iverilog Not_test.v Not.v  // Generate Not_test.vcd
gtkwave Not_test.vcd  // See graphical wave output.

/*
 * And gate.
 */
iverilog -o And_test.vvp And_test.v And.v Not.v  // Generate And_test.vvp
vvp And_test.vvp  // Execute.
iverilog And_test.v And.v Not.v  // Generate And_test.vcd
gtkwave And_test.vcd  // See graphical wave output.

/*
 * Or gate.
 */
iverilog -o Or_test.vvp Or_test.v Or.v Not.v  // Generate Or_test.vvp
vvp Or_test.vvp  // Execute.
iverilog Or_test.v Or.v Not.v  // Generate Or_test.vcd
gtkwave Or_test.vcd  // See graphical wave output.

/*
 * Xor gate.
 */
iverilog -o Xor_test.vvp Xor_test.v Xor.v Not.v
vvp Xor_test.vvp
iverilog Xor_test.v Xor.v Not.v
gtkwave Xor_test.vcd

/*
 * Multiplexor
 */
iverilog -o Mux_test.vvp Mux_test.v Mux.v Not.v
vvp Mux_test.vvp
iverilog Mux_test.v Mux.v Not.v
gtkwave Mux_test.vcd

/*
 * Demultiplexor.
 */
iverilog -o DMux_test.vvp DMux_test.v DMux.v And.v Not.v
vvp DMux_test.vvp
iverilog DMux_test.v DMux.v And.v Not.v
gtkwave DMux_test.vcd

/*
 * Not16.
 */
iverilog -o Not16_test.vvp Not16_test.v Not16.v Not.v
vvp Not16_test.vvp
iverilog Not16_test.v Not16.v Not.v
gtkwave Not16_test.vcd

/*
 * And16.
 */
iverilog -o And16_test.vvp And16_test.v And16.v And.v Not.v
vvp And16_test.vvp
iverilog And16_test.v And16.v And.v Not.v
gtkwave And16_test.vcd

/*
 * Or16.
 */
iverilog -o Or16_test.vvp Or16_test.v Or16.v Or.v Not.v
vvp Or16_test.vvp
iverilog Or16_test.v Or16.v Or.v Not.v
gtkwave Or16_test.vcd

/*
 * Mux16.
 */
iverilog -o Mux16_test.vvp Mux16_test.v Mux16.v Mux.v Not.v
vvp Mux16_test.vvp
iverilog Mux16_test.v Mux16.v Mux.v Not.v
gtkwave Mux16_test.vcd

/*
 * Or8Way.
 */
iverilog -o Or8Way_test.vvp Or8Way_test.v Or8Way.v Or.v Not.v
vvp Or8Way_test.vvp
iverilog Or8Way_test.v Or8Way.v Or.v Not.v
gtkwave Or8Way_test.vcd

/*
 * 16-bit, 4-way multiplexor.
 */
iverilog -o Mux4Way16_test.vvp Mux4Way16_test.v Mux4Way16.v Mux16.v Mux.v Not.v
vvp Mux4Way16_test.vvp
iverilog Mux4Way16_test.v Mux4Way16.v Mux16.v Mux.v Not.v
gtkwave Mux4Way16_test.vcd

/*
 * 16-bit, 8-way multiplexor.
 */
iverilog -o Mux8Way16_test.vvp Mux8Way16_test.v Mux8Way16.v Mux4Way16.v Mux16.v Mux.v Not.v
vvp Mux8Way16_test.vvp
iverilog Mux8Way16_test.v Mux8Way16.v Mux4Way16.v Mux16.v Mux.v Not.v
gtkwave Mux8Way16_test.vcd

